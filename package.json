{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for pager app",
  "main": "index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "knex": "^0.20.7",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-sorted": "^0.2.0",
    "mocha": "^7.0.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start": "node listen.js",
    "setup-dbs": "psql -f ./db/setup.sql",
    "seed": "knex seed:run",
    "seed-test": "NODE_ENV=test knex seed:run",
    "test-utils": "mocha spec/utils.spec.js",
    "test": "mocha spec/server.spec.js",
    "reset-db": "psql -f db/setup.sql",
    "migrate-make": "knex migrate:make",
    "migrate-latest": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "seed-prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex seed:run",
    "migrate-latest:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:latest",
    "migrate-rollback:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:rollback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joecooper1/pager-backend.git"
  },
  "keywords": [],
  "author": "Joe Cooper",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joecooper1/pager-backend/issues"
  },
  "homepage": "https://github.com/joecooper1/pager-backend#readme"
}
